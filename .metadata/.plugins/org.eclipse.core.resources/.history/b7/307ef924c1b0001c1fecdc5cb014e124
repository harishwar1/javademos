package arrays;

public class revarr {
	public static void main(String[] args) {
		int[] arr= {1,5,3,7,11,9,15};
		revarr();
		
	}

	private static void revarr() {
		// TODO Auto-generated method stub
		//int[] arr2= new int[];
		for(int i=6;i>=0;i-- ) {
			
			System.out.println(arr[i]);
		}
	}

}
